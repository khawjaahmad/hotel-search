# Firebase Test Lab Configuration
# Copy this file to .env and fill in your values

# Firebase Project ID
FIREBASE_PROJECT_ID=your-firebase-project-id

# SerpAPI Key for hotel search functionality
SERPAPI_API_KEY=your-serpapi-key

# Firebase Test Lab Settings
FIREBASE_RESULTS_BUCKET=your-project-test-results
FIREBASE_TIMEOUT=15m

# Test Configuration
TEST_TIMEOUT=600
MAX_PARALLEL_TESTS=4
SCREENSHOT_ON_FAILURE=true

# Device Configuration
DEFAULT_ANDROID_DEVICE=shiba
DEFAULT_ANDROID_VERSION=34
DEFAULT_IOS_DEVICE=iphone15pro
DEFAULT_IOS_VERSION=18.0

# GitHub Actions Configuration (for reference)
# These should be set as GitHub Secrets, not in .env file
# GOOGLE_CLOUD_SERVICE_ACCOUNT_KEY={"type": "service_account", ...}

# Debug and Development
DEBUG=false
VERBOSE_LOGGING=false